{
  "name": "lla-api",
  "version": "0.0.1",
  "description": "api serverless api.",
  "private": true,
  "scripts": {
    "test": "jest --env=node",
    "build": "tsc && tsc-alias",
    "local": "LBERTAPP_ENV=local sls offline start --stage local",
